<div class="container">
  <div class="row">
    <div class="span12">
      <% if alert %><div class="alert alert-error fade in"><a class="close" data-dismiss="alert">×</a><%= alert %></div><% end %>
      <% if notice %><div class="alert alert-info fade in"><a class="close" data-dismiss="alert">×</a><%= notice %></div><% end %>
    </div>
  </div>
</div>
